,CAAC,CAAC,CAAC,CAAC,EAAE;EACb,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;EACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC3B;AACA,CAAC,CAAC,CAAC,EAAE;EACH,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;EACrC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;;EAEV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;EAC3C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;AACpB","file":"/var/www/html/thien_an/thien-an-salon-website/src/views/elements/banHangVaMarketing/quanLyDichVu/capNhatDichVu.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"col-md-10 res mb-5 mt-3\">\n    <h5 class=\"mt-3 font-weight-bold color-main\">CẬP NHẬT DỊCH VỤ</h5>\n    <!-- Thông tin salon -->\n    <div class=\"mt-4 bg-8 pt-3 pl-5 pr-5\">\n      <h6>THÔNG TIN DỊCH VỤ</h6>\n      <div class=\"mt-3 pb-2\">\n        <table class=\"table table-borderless\">\n          <tbody class=\"text-right\">\n            <tr>\n              <td style=\"width: 15%\">\n                <div class=\"table-input\">Ảnh dịch vụ:</div>\n              </td>\n              <td style=\"width: 35%\">\n                <div v-if=\"salon.image == ''\" class=\"text-center\">\n                  <input\n                    type=\"file\"\n                    id=\"upload\"\n                    hidden\n                    @change=\"onFileChange\"\n                  />\n                  <label\n                    class=\"bg1 hover mt-3 text-center p-2 pl-4 pr-4 color-main\"\n                    style=\"cursor: pointer\"\n                    for=\"upload\"\n                    >Thêm ảnh</label\n                  >\n                </div>\n                <div\n                  v-else\n                  class=\"bg\"\n                  id=\"preview\"\n                  style=\"\n                    cursor: pointer;\n                    justify-content: center;\n                    align-items: center;\n                    display: flex;\n                    position: relative;\n                  \"\n                >\n                  <img\n                    :src=\"salon.image\"\n                    alt=\"\"\n                    style=\"width: 150px; border-radius: 8px\"\n                  />\n                  <img\n                    src=\"../../../../assets/image/xx.png\"\n                    alt=\"\"\n                    style=\"position: absolute; right: 75px; top: -5px\"\n                    @click=\"removeImage()\"\n                  />\n                </div>\n              </td>\n            </tr>\n            <tr>\n              <td style=\"width: 15%\">\n                <div class=\"table-input\">Tên dịch vụ:</div>\n              </td>\n              <td style=\"width: 35%\">\n                <input\n                  type=\"text\"\n                  placeholder=\"Tên dịch vụ... \"\n                  class=\"w-100\"\n                  v-model=\"salon.title\"\n                  required\n                />\n              </td>\n              <td style=\"width: 15%\">\n                <div class=\"table-input\">Giá:</div>\n              </td>\n              <td style=\"width: 35%\">\n                <div class=\"right-inner-addon input-container\">\n                  <span>VNĐ</span>\n                  <input\n                    type=\"number\"\n                    min=\"0\"\n                    placeholder=\"Giá... \"\n                    class=\"w-100\"\n                    v-model=\"salon.price\"\n                    required\n                  />\n                </div>\n              </td>\n            </tr>\n            <tr>\n              <td style=\"width: 15%\">\n                <div class=\"table-input\">Mô tả:</div>\n              </td>\n              <td colspan=\"3\">\n                <textarea\n                  class=\"w-100\"\n                  rows=\"6\"\n                  v-model=\"salon.description\"\n                ></textarea>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n      <div class=\"mt-3 text-center pb-3\">\n        <button class=\"borderx mr-2 color-main\" @click=\"backPage\">\n          Quay lại\n        </button>\n        <span @click=\"updateService\" class=\"borderx color-main\">\n          Lưu và hiển thị\n        </span>\n      </div>\n    </div>\n    <!-- ========================== -->\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/main\";\nexport default {\n  name: \"themSalon\",\n  data() {\n    return {\n      detailService: {},\n      salon: {\n        title: \"\",\n        description: \"\",\n        price: \"\",\n        image: \"\",\n      },\n        serviceId: \"\",\n        salonId: \"\",\n      errors: [],\n      url: \"\",\n    };\n  },\n  created() {\n    this.serviceId = this.$route.params.updateId;\n    this.salonId = this.$route.params.salonId;\n  },\n  beforeMount() {\n    this.getService();\n  },\n  methods: {\n    getService() {\n      HTTP.get(\n        `/salon/main/manage/service/${this.serviceId}?salonId=${this.salonId}`\n      ).then((res) => {\n        let result = res.data;\n        if (result.status === 1) {\n          let data = result;\n          this.salon = data.data;\n          console.log(this.salon);\n        }\n        this.loading = false;\n      });\n    },\n    updateService() {\n      let formData = new FormData();\n      formData.append(\"title\", this.salon.title);\n      formData.append(\"description\", this.salon.description);\n      formData.append(\"price\", this.salon.price);\n      formData.append(\"salonId\", this.salonId);\n      formData.append(\"serviceId\", this.serviceId);\n      formData.append(\"image\", this.url);\n      HTTP.post(`/salon/main/manage/service/update`, formData).then((res) => {\n        let result = res.data;\n        if (result.status === 1) {\n          this.$swal(\"\", result.message, \"success\");\n          setTimeout(function() {\n            history.back();\n          }, 1000);\n        } else {\n          this.$swal(\"\", result.message, \"error\");\n        }\n      });\n    },\n    removeImage() {\n      this.salon.image = \"\";\n    },\n    onFileChange(e) {\n      this.url = e.target.files[0];\n      this.salon.image = URL.createObjectURL(this.url);\n    },\n    backPage() {\n      history.back();\n    },\n  },\n};\n</script>\n<style scoped>\n* {\n  font-size: 14px;\n}\n.table-input {\n  padding: 5px 10px;\n}\n.borderx {\n  border: 1px solid #d4476f;\n  padding: 5px 25px;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.borderx:hover {\n  color: white;\n  background-color: #d4476f;\n}\n.bg1 {\n  background: rgba(255, 255, 255, 0.56);\n  /* shape */\n\n  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.05);\n  border-radius: 8px;\n}\n</style>\n"]}]}