in doanhSo\" :key=\"index\">\n              <td style=\"text-align: right !important\" class=\"pt-2\">\n                <img\n                  :src=\"item.avatar\"\n                  alt=\"\"\n                  style=\"width: 33px; height: 33px; border-radius: 50%\"\n                />\n              </td>\n              <td class=\"pl-4\">{{ item.name }}</td>\n              <td class=\"text-center\">{{ item.sex }}</td>\n              <td class=\"text-center\">{{ item.phone }}</td>\n              <td>{{ item.address }}</td>\n              <td class=\"color-main\">{{ formatPrice(item.total_money) }}</td>\n              <td>\n                <router-link\n                  :to=\"{\n                    name: 'chiTietKhachHang',\n                    params: { custommerId: item.id },\n                  }\"\n                >\n                  <span class=\"color-main\">Chi tiết</span>\n                </router-link>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/main\";\nimport loadingData from \"@/components/loadingData\";\nimport { formatPrice } from \"@/main\";\nexport default {\n  name: \"khachHangDoanhSo\",\n  components: {\n    loadingData,\n  },\n  data() {\n    return {\n      msg: \"\",\n      loading: true,\n      hasData: false,\n      doanhSo: {},\n    };\n  },\n  beforeMount() {\n    this.listDoanhSo();\n  },\n  methods: {\n    listDoanhSo() {\n      HTTP.get(`/salon/main/manage/managerCustomer/2`).then((res) => {\n        let result = res.data;\n        // console.log(result);\n        if (result.status === 1) {\n          this.loading = false;\n          this.hasData = true;\n          let data = result.data.data;\n          // console.log(data);\n          if (data.length > 0) {\n            this.doanhSo = data;\n            // console.log(this.doanhSo);\n          } else {\n            this.loading = false;\n            this.hasData = true;\n            this.msg = \"Chưa có khách hàng!!!\";\n          }\n        } else {\n          this.notification.status = true;\n          this.notification.message = result.message;\n        }\n      });\n    },\n    // ================\n    formatPrice,\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}