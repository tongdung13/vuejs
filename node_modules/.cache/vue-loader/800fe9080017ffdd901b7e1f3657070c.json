AAC,CAAC,CAAC,CAAC,CAAC;;MAEvC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAC5B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACnE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE;UACP,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACrB,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE;YACvB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC3C,EAAE,CAAC,CAAC,CAAC,EAAE;YACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACzC;QACF;MACF,CAAC;IACH,CAAC;IACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;OACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACjB,CAAC;EACH,CAAC;AACH,CAAC","file":"/var/www/html/thien_an/thien-an-salon-website/src/views/elements/taiChinhVaKeToan/baoCaoMuaHang/thembaoCaoMuaHang.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"col-md-10 mb-5 mt-3\">\n    <h5 class=\"mt-3 font-weight-bold color-main\">THÊM BÁO CÁO MUA HÀNG</h5>\n    <!-- Thông tin salon -->\n    <div class=\"mt-4 bg-8 pt-3 pl-5 pr-5\">\n      <form @submit.prevent=\"addReportBill\">\n        <h6>THÊM BÁO CÁO MUA HÀNG</h6>\n        <div class=\"mt-3 pb-2\">\n          <table class=\"table table-borderless\">\n            <tbody class=\"text-right\">\n              <tr>\n                <td style=\"width: 15%\">\n                  <div class=\"table-input\">Đơn vị bán hàng:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    placeholder=\"Đơn vị... \"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.seller\"\n                  />\n                </td>\n                <td>\n                  <div class=\"table-input\">Địa chỉ:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    class=\"w-100\"\n                    placeholder=\"Địa chỉ...\"\n                    required\n                    v-model=\"reportBill.address\"\n                  />\n                </td>\n              </tr>\n              <tr>\n                <td>\n                  <div class=\"table-input\">Mã số thuế:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    placeholder=\"Mã số thuế...\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.tax_code\"\n                  />\n                </td>\n                <td>\n                  <div class=\"table-input\">Người giao hàng:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    placeholder=\"Người giao hàng...\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.shipper\"\n                  />\n                </td>\n              </tr>\n              <tr>\n                <td style=\"width: 15%\">\n                  <div class=\"table-input\">Sản phẩm:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    placeholder=\"Tên sản phẩm...\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.producer\"\n                  />\n                </td>\n              </tr>\n\n              <tr>\n                <td>\n                  <div class=\"table-input\">Nghiệp vụ:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    class=\"w-100\"\n                    placeholder=\"Nnghiệp vụ...\"\n                    required\n                    v-model=\"reportBill.business\"\n                  />\n                </td>\n\n                <td>\n                  <div class=\"table-input\">Số tờ hoá đơn:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"text\"\n                    placeholder=\"Số hoá đơn...\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.number\"\n                  />\n                </td>\n              </tr>\n              <tr>\n                <td>\n                  <div class=\"table-input\">Ngày nhận hoá đơn:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"date\"\n                    placeholder=\"Ngày...\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.bill_date\"\n                  />\n                </td>\n                 <td>\n                  <div class=\"table-input\">Ngày nhập hoá đơn:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"date\"\n                    placeholder=\"Ngày...\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.input_date\"\n                  />\n                </td>\n              </tr>\n              <tr>\n                 <td>\n                  <div class=\"table-input\">Ngày thanh toán:</div>\n                </td>\n                <td>\n                  <input\n                    type=\"date\"\n                    class=\"w-100\"\n                    required\n                    v-model=\"reportBill.payment_date\"\n                  />\n                </td>\n              </tr>\n\n              <tr>\n                <td>\n                  <div class=\"table-input\">Salon:</div>\n                </td>\n                <td>\n                  <select v-model=\"reportBill.salon_id\" class=\"input w-100\" @change=\"changeStore()\">\n                    <option value=\"\">--Chọn Salon--</option>\n                    <option\n                      v-for=\"(salon, index) in listSalon\"\n                      :key=\"index\"\n                      :value=\"salon.id\"\n                    >\n                      {{ salon.salon_name }}\n                    </option>\n                  </select>\n                </td>\n\n                <td>\n                  <div class=\"table-input\">Quỹ tài khoản:</div>\n                </td>\n                <td>\n                  <select v-model=\"fund\" v-if=\"reportBill.salon_id\" class=\"input w-100\" >\n                    <option value=\"\">--- Chọn tài khoản ---</option>\n                    <option\n                      v-for=\"fund in listFunds\"\n                      :key=\"fund.id\"\n                      :value=\"fund\"\n                    >\n                      {{ fund.account_number }} - {{ fund.bank }}\n                    </option>\n                  </select>\n                  <div v-else class=\"inputx text-left\">\n                    --- Chọn tài khoản ---\n                  </div>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n        <div class=\"mt-3 text-center pb-3\">\n          <span @click=\"historyBack\" class=\"borderx mr-2 color-main\"\n            >Quay lại</span\n          >\n          <button type=\"submit\" class=\"borderx color-main\">\n            Lưu và hiển thị\n          </button>\n        </div>\n      </form>\n    </div>\n    <!-- ========================== -->\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/main\";\nexport default {\n  name: \"themBaoCaoMuaHang\",\n  data() {\n    return {\n      reportBill: {\n        fund_id: \"\",\n        salon_id: \"\"\n      },\n      errors: [],\n      listSalon: {},\n      listFunds: [],\n      fund: {},\n    };\n  },\n  created() {\n    HTTP.get(`/salon/main/manage/branch/all`).then((res) => {\n      this.listSalon = res.data.data;\n      // console.log(this.listSalonId);\n    });\n  },\n  methods: {\n    changeStore(){\n      this.listFund();\n    },\n     listFund() {\n      HTTP.get(\"/salon/main/manage/form/\" + this.reportBill.salon_id + \"/funds\")\n        .then((res) => {\n          var result = res.data;\n          if (result.status === 1) {\n            this.listFunds = result.data;\n            // console.log(this.listFunds);\n          }\n        })\n        .catch((e) => {\n          this.errors.push(e);\n        });\n    },\n    addReportBill() {\n       this.reportBill.fund_id = this.fund.id;\n\n      console.log(this.reportBill);\n      HTTP.post(`/salon/main/manage/billGoods/store`, this.reportBill).then(\n        (res) => {\n          let result = res.data;\n          if (result.status === 1) {\n            this.$swal(\"\", result.message, \"success\");\n          } else {\n            this.$swal(\"\", result.message, \"error\");\n          }\n        }\n      );\n    },\n    historyBack() {\n       history.back();\n    },\n  },\n};\n</script>\n<style scoped>\n* {\n  font-size: 14px;\n}\n.table-input {\n  padding: 5px 10px;\n}\n.borderx {\n  border: 1px solid #d4476f;\n  padding: 5px 25px;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.borderx:hover {\n  color: white;\n  background-color: #d4476f;\n}\n</style>\n"]}]}